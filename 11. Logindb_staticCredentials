//DB 
CREATE DATABASE logindb;
USE logindb;

CREATE TABLE loginTable(uname varchar(20), pass varchar(20));

INSERT INTO loginTable values('Jenita', '1234');

SELECT * FROM loginTable WHERE uname = 'Jenita' and pass = '1234';
